{
    "manifest_version": 2,
    "name": "URL Shortener",
    "version": "1.0",
    "description": "Shorten URLs for easy sharing.",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "https://api.shrtco.de/"]
  }
  